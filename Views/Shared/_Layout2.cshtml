    <!-- Header -->
    <header id="cabecalho" class="intro-header">
        <div class="container">
            <div class="row">
                <div class="col-lg-12">
                    <div class="intro-message">
                        <h1>EasyBanca</h1>
                        <h3 style="background-color:#06cc20; opacity:0.7; border-radius:5px; padding:20px;">QUER ENCONTRAR OS MELHORES TUTORES PARA REFORÇO ESCOLAR?</h3>
                        <!--<h4 for="txtEmail" class="text-primary">Informe seu email abaixo!</h4>-->
                        <hr class="intro-divider" style="margin-top:0px;">
                        <article class="col-md-12">
                            <article class="col-md-4"></article>
                            <article class="col-md-4 text-center">
                                <form>
                                    <input id="txtEmail" placeholder="seuemail@provedor.com" type="text" autofocus class="form-control input-lg" />
                                    <br />
                                    <article style="background-color:#fff; color:#0c5f17; padding:2px; border-radius:10px; opacity:0.8;">
                                        <label style="font-weight:bold;" class="radio-inline"><input type="radio" checked name="optradio">Sou Aluno</label>
                                        <label style="font-weight:bold;" class="radio-inline"><input type="radio" name="optradio">Sou Pai/Mãe</label>
                                        <label style="font-weight:bold;" class="radio-inline"><input type="radio" name="optradio">Sou Tutor</label>
                                    </article>
                                    <br />
                                    <button onclick="enviarEmail();" class="btn input-lg btn-default btn-success form-control" style="font-family:Verdana; font-weight:bold; font-size:20px;">Envie seu email</button>
                                </form>
                            </article>
                            <article class="col-md-4"></article>
                        </article>
                        <article class="col-md-12">
                            <hr class="intro-divider">
                            <ul class="list-inline intro-social-buttons">
                                <li>
                                    <a href="https://twitter.com/easybanca" target="_blank" class="btn btn-default btn-lg"><i class="fa fa-twitter fa-fw"></i> <span class="network-name">Twitter</span></a>
                                </li>
                                <li>
                                    <a href="https://www.facebook.com/easybanca/" target="_blank" class="btn btn-default btn-lg"><i class="fa fa-fw"><img src="~/Content/img/facebook.png" width="20" /></i> <span style="padding-left:10px;" class="network-name">Facebook</span></a>
                                </li>
                                <li>
                                    <a href="https://www.linkedin.com/company/easybanca" target="_blank" class="btn btn-default btn-lg"><i class="fa fa-linkedin fa-fw"></i> <span class="network-name">Linkedin</span></a>
                                </li>
                                <li>
                                    <a href="https://www.instagram.com/easybanca/" target="_blank" class="btn btn-default btn-lg"><i class="fa fa-fw"><img src="~/Content/img/instagram.png" width="20" /></i> <span style="padding-left:10px;" class="network-name">Instagram</span></a>
                                </li>
                            </ul>
                        </article>
                    </div>
                </div>
            </div>
        </div>
        <!-- /.container -->
    </header>
