@if (null != Session[EasyBanca.Models.WebUtil.Constantes.SESSAO_MENSAGEM])
{
    <article class="bg-success">
        @Session[EasyBanca.Models.WebUtil.Constantes.SESSAO_MENSAGEM].ToString()
    </article>
